cout<<"masukan kelas"<<endl;
cin>>kelas;

cout<<"masukan nim"<<endl;
cin>>nim;

cout<<"masukan hobi"<<endl;
cin>>hobi;

cout<<"==data diri=="<<endl;
cout<<"nama:"<<nama<<endl;
cout<<"kelas:"<<kelas<<endl;
cout<<"nim:"<<nim<<endl;
cout<<"hobi:"<<hobi<<endl;
}
